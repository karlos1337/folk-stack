{
  "name": "folk-stack-template",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "remix:dev": "remix dev",
    "dev": "npm run css & docker-compose up",
    "start": "remix-serve build",
    "css:sass": "sass --load-path=node_modules --embed-sources styles/:.cache/tmp/css",
    "css:sass:watch": "npm run css:sass -- --watch",
    "css:postcss": "postcss '.cache/tmp/css/**/*.css' --base .cache/tmp/css --dir app/styles/",
    "css:postcss:watch": "npm run css:postcss -- --watch",
    "css": "concurrently \"npm:css:sass:watch\" \"npm:css:postcss:watch\"",
    "svgs": "svgpack icons > styles/core/_svgs.scss",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test dev http://localhost:3000 \"npx cypress run --headless\"",
    "prisma": "dotenv-flow -- docker exec remix npx prisma",
    "prisma:schema": "dotenv-flow -- docker exec remix npx ts-node prisma/_schema.ts"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.5",
    "@prisma/client": "^3.14.0",
    "@remix-run/node": "*",
    "@remix-run/react": "*",
    "@remix-run/serve": "*",
    "bootstrap": "^5.1.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@marsbased/svgpack": "^1.0.3",
    "@remix-run/dev": "*",
    "@remix-run/eslint-config": "*",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/react": "^18.0.10",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@vitejs/plugin-react": "^1.3.2",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.1",
    "cssnano": "^5.1.10",
    "cypress": "^8.0.2",
    "dotenv-flow": "^3.2.0",
    "dotenv-flow-cli": "^1.0.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "happy-dom": "^5.0.0",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "prettier": "^2.6.2",
    "prisma": "^3.14.0",
    "sass": "^1.52.1",
    "schemix": "^0.5.12",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^14.8.5",
    "stylelint-config-twbs-bootstrap": "^4.0.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.2",
    "vite": "^2.9.9",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.13.1"
  },
  "engines": {
    "node": ">=14"
  }
}
